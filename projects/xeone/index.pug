    //- Creative Work 
    section.creativeWork(id='creativeWork')
      .container
        .row(class='works justify-content-center')
          div(class='col-xs-12 col-lg-6')
            img(class='img-fluid' src='img/creativeWork/portfolio-1.jpg')

          div(class='col-xs-12 col-lg-6 text-center')
