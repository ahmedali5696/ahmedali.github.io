  body
  
    //- Navigation Bar
    nav(class='navbar navbar-expand-lg navbar-light')
      .container
        a(class='navbar-brand d-sm-inline-block d-lg-none' href='#')
            h1
              span Xe
              .
                One
              span .
        button.navbar-toggler(data-toggle='collapse' data-target='#phoneNavbar' aria-controls='phoneNavbar')
          i(class='fa fa-bars')
        
        div(class='navbar-collapse collapse' id='phoneNavbar')
          ul(class='navbar-nav mr-auto')
            - var navLiLeft= ['Home', 'Features', 'Team', 'Portofilo']
            each liItems in navLiLeft
              li(class='nav-item mr-5')
                a.nav-link(href='#') #{liItems}
          
          a(class='navbar-brand d-lg-inline-block d-sm-none' href='#')
            h1
              span Xe
              .
                One
              span .

          ul(class='navbar-nav ml-auto')
            - var navLiRight= ['Packages', 'Clients', 'Blog', 'Contant']
            each liItems in navLiRight
              li(class='nav-item ml-5')
                a.nav-link(href='#') #{liItems}
    
    //- Header
    header#header
      div(class='carousel slide header' data-ride='carousel' id='carouselHeader')
        ol(class='carousel-indicators flex-column d-none d-md-flex')
          - for (var i = 0; i <= 2; i++)
            li(data-target='#carouselHeader' data-slide-to=i)
          li.active(data-target='#carouselHeader' data-slide-to='3')
        div.carousel-inner
          - for (var i = 0; i <= 2; i++)
            div(class='carousel-item text-center')
              p The Ultimate
              h2.h1 Next Big Thing                   
              p In One Page
              h6 The Best Multipurpose One Page Template in Market
              button.text-uppercase Learn More
          div(class='carousel-item active text-center')
              p The Ultimate
              h2.h1 Next Big Thing                   
              p In One Page
              h6 The Best Multipurpose One Page Template in Market
              button.text-uppercase Learn More

