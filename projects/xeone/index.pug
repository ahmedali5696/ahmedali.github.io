      h2.text-center(class='slideanim') Mobile Applications

      .container
        .row(class='justify-content-between mt-5')
          div(class='text-right col-xs-12 col-md-4')
            - for (var i = 1; i <= 3; i++)
              div(class='mobItem slideanim d-flex mb-5 mt-4')
